# Uncomment the next line to define a global platform for your project
platform :ios, '11.4'

# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

def all_pods
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'netfox', '~> 1.15.0'
  pod 'ActionSheetPicker-3.0'
  pod 'IQKeyboardManagerSwift'
  pod 'ActiveLabel'
  pod 'DeviceKit'
  pod 'HandyJSON'
  pod 'RealmSwift'
  pod 'SwiftyJSON'
  pod 'SwiftDate'
  pod 'SnapKit'
  pod 'Disk'
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/RemoteConfig'
  pod 'FirebaseMessaging'
  pod 'SwiftMessages'
  pod 'SVProgressHUD', '~> 2.2'
  pod 'SwiftLint'
  pod 'RxSwift'
  pod 'RxCocoa'
end

target 'git-sample-ios' do
    all_pods
end

target 'git-sample-ios-stg' do
    all_pods
end
